<p id="notice"><%= notice %></p>

<h1>Publisher Bookmarks</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Publisher</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @publisher_bookmarks.each do |publisher_bookmark| %>
      <tr>
        <td><%= publisher_bookmark.user %></td>
        <td><%= publisher_bookmark.publisher %></td>
        <td><%= link_to 'Show', publisher_bookmark %></td>
        <td><%= link_to 'Edit', edit_publisher_bookmark_path(publisher_bookmark) %></td>
        <td><%= link_to 'Destroy', publisher_bookmark, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publisher Bookmark', new_publisher_bookmark_path %>
